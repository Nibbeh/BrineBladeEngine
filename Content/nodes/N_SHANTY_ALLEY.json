{
  "Id": "N_SHANTY_ALLEY",
  "Title": "Shanty Alley",
  "Description": "Lean-tos and whisper-trades. A chalk fishbone marks a back door.",
  "Paragraphs": [
    "A child brushes your belt. Footsteps gather behind you."
  ],
  "Options": [
    {
      "Text": "Let the child keep the coin (-1g).",
      "Effects": [
        {
          "Op": "addGold",
          "Amount": -1
        },
        {
          "Op": "setFlag",
          "Name": "moral.kind.child",
          "Value": true
        }
      ]
    },
    {
      "Text": "Scold and take it back.",
      "Effects": [
        {
          "Op": "setFlag",
          "Name": "moral.harsh.child",
          "Value": true
        }
      ]
    },
    {
      "Text": "Face the footfalls (ambush).",
      "Effects": [
        {
          "Op": "startCombat",
          "Id": "ENEMY_RAT_SWARM"
        }
      ]
    },
    {
      "Text": "Enter the Smugglers’ Den.",
      "Effects": [
        {
          "Op": "goto",
          "Id": "N_SMUGGLERS_DEN"
        }
      ]
    },
    {
      "Text": "Back to the square.",
      "Effects": [
        {
          "Op": "goto",
          "Id": "N_HARBOR_SQUARE"
        }
      ]
    }
  ],
  "Exits": [
    {
      "Text": "To Smugglers’ Den",
      "To": "N_SMUGGLERS_DEN"
    },
    {
      "Text": "Back to Harbor Town Square",
      "To": "N_HARBOR_SQUARE"
    }
  ]
}